{
  "name": "@longfor/mara",
  "version": "1.2.0",
  "description": "mara是一个前端日志系统, 它将错误信息记录于本地localStorage中。无任何依赖、无入侵性。",
  "main": "./dist/mara.esm.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "DEBUG=true rollup -c -w --host localhost.longfor.com --port 443",
    "build": "rm -rf ./dist && rollup -c && rollup --config rollupExternal.config.js",
    "dev": "serve",
    "lint": "eslint ./src",
    "lintfix": "eslint ./src --fix"
  },
  "repository": {
    "type": "git",
    "url": "git@git.longhu.net:8010/flowdata/mara.git"
  },
  "author": "TNFE",
  "license": "ISC",
  "keywords": [
    "situ / felog / tntlog / log / frontendlog"
  ],
  "files": [
    "dist",
    "package.json",
    "README.md",
    ".gitignore",
    ".npmignore"
  ],
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.18.6",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^18.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "eslint": "^7.26.0",
    "eslint-plugin-prettier": "^3.4.0",
    "rollup": "^2.47.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-import-css": "^3.0.3",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "ali-oss": "^6.17.1",
    "axios": "^0.27.2",
    "clipboard": "^2.0.11",
    "file-saver": "^2.0.5",
    "nanoid": "^3.3.4",
    "sweetalert2": "^11.4.14",
    "toastify-js": "^1.11.2"
  }
