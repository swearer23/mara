{
  "name": "@longfor/mara",
  "version": "2.1.1",
  "description": "mara是一个前端日志系统, 它将错误信息上报至ES存储",
  "main": "./dist/mara.umd.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "DEBUG=true rollup -c -w",
    "dev": "vite --port 8888 --host 0.0.0.0",
    "release": "node scripts/release.mjs",
    "build": "rm -rf ./dist && rollup -c",
    "lint": "eslint ./src",
    "lintfix": "eslint ./src --fix",
    "test:unit": "jest",
    "test:e2e": "npx playwright test && npx playwright show-report"
  },
  "repository": {
    "type": "git",
    "url": "git@git.longhu.net:8010/flowdata/mara.git"
  },
  "author": "TNFE",
  "license": "ISC",
  "keywords": [
    "situ / felog / tntlog / log / frontendlog"
  ],
  "files": [
    "dist",
    "package.json",
    "README.md",
    ".gitignore",
    ".npmignore"
  ],
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/preset-env": "^7.18.9",
    "@playwright/test": "^1.24.1",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^18.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "babel-jest": "^28.1.3",
    "eslint": "^7.26.0",
    "eslint-plugin-prettier": "^3.4.0",
    "inquirer": "^9.1.2",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "rollup": "^2.47.0",
    "rollup-plugin-import-css": "^3.0.3",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "vite": "^3.0.6"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "crypto-js": "^4.1.1",
    "nanoid": "^3.3.4"
  }
}